#include <bits/stdc++.h>

int lengthOfLongestConsecutiveSequence(vector<int> &arr, int n) {
    // Write your code here.
    sort(arr.begin(),arr.end());
    int ans=1;
    int temp=1;
    for(int i=1;i<n;i++){
        if(arr[i]==arr[i-1]){
            continue;
        }
        else if((arr[i]==(arr[i-1]+1))){
            temp++;
            
        }
        else{
            temp=1;

        }
       ans=max(temp,ans); 
    }
    return ans;
}
